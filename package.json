{
  "name": "pro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode dev",
    "build-test": "vue-cli-service build --mode proTest",
    "build": "vue-cli-service build --mode prod",
    "lint": "vue-cli-service lint",
    "start": "node index.js",
    "server": "nodemon index.js --ignore client"
  },
  "dependencies": {
    "@antv/g6": "3.4.7",
    "@fullcalendar/core": "^4.4.0",
    "@fullcalendar/daygrid": "^4.4.0",
    "@fullcalendar/interaction": "^4.4.0",
    "@fullcalendar/timegrid": "^4.4.0",
    "@fullcalendar/vue": "^4.4.0",
    "@types/three": "^0.146.0",
    "ali-oss": "^6.16.0",
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.6.5",
    "default-passive-events": "^2.0.0",
    "dhtmlx-gantt": "^7.0.13",
    "element-ui": "^2.15.0",
    "exif-js": "^2.3.0",
    "file-saver": "^2.0.5",
    "js-md5": "^0.7.3",
    "jszip": "^3.7.1",
    "lodash": "^4.17.21",
    "node-sass": "^8.0.0",
    "nprogress": "^0.2.0",
    "qs": "^6.9.6",
    "sass-loader": "^7.0.0",
    "three": "^0.146.0",
    "vue": "^2.6.11",
    "vue-fragment": "^1.5.1",
    "vue-grid-layout": "^2.3.12",
    "vue-qriously": "^1.1.1",
    "vue-router": "^3.5.1",
    "vue-worker": "^1.2.1",
    "vue2-org-tree": "^1.3.5",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.6.2",
    "wangeditor": "^4.6.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "compression-webpack-plugin": "6.1.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^4.1.3",
    "less-loader": "^7.3.0",
    "sass": "^1.32.8",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.11",
    "worker-loader": "^3.0.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint",
      "ecmaFeatures": {
        "legacyDecorators": true
      }
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ],
      "no-empty": [
        "error",
        {
          "allowEmptyCatch": true
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
